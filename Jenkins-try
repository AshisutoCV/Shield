node {
    
    parameters {
        string(name: 'BranchName',
            defaultValue: 'Dev',
            description: "Give me this branch name")
    }    
    
    stage("Test param") {
        echo "This is ${params.BarnchName}"
    }


    stage("Get latest version") {
        git url: "https://github.com/EricomSoftwareLtd/Shield.git",  credentialsId: "github-ozlevka", branch: "${params.BarnchName}"
        echo "Fetch all data from github ${params.BarnchName}"
    }
    
}