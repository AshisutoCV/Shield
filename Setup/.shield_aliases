#New Commands
alias shield-status='sudo /usr/local/ericomshield/status.sh -a'
alias shield-start='sudo /usr/local/ericomshield/start.sh'
alias shield-restart='sudo /usr/local/ericomshield/restart.sh'
alias shield-stop='sudo /usr/local/ericomshield/stop.sh'
alias shield-status-nodes='sudo /usr/local/ericomshield/addnodes.sh --node-status'
alias shield-status-services='sudo /usr/local/ericomshield/addnodes.sh --status'
alias shield-version='sudo /usr/local/ericomshield/showversion.sh'
