alias shield_status='sudo /usr/local/ericomshield/status.sh -a'
alias shield_start='sudo /usr/local/ericomshield/run.sh'
alias shield_restart='sudo /usr/local/ericomshield/restart.sh'
alias shield_stop='sudo /usr/local/ericomshield/stop.sh'
alias shield_node-status='sudo /usr/local/ericomshield/ericomshield-setup-node.sh --node-status'
alias shield_service-status='sudo /usr/local/ericomshield/ericomshield-setup-node.sh --status'
alias shield_version='sudo /usr/local/ericomshield/showversion.sh' 
